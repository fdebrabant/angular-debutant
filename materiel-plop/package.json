{
  "name": "materiel-plop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --proxy-config proxy.conf.json\" \"node ./server/main.js\"",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --stats-json=./dist/stats.json",
    "serve:dist": "concurrently \"http-server -g 1 -c-1 -o --proxy http://localhost:8081 -p 9001 ./dist\" \"node ./server/main.js\"",
    "serve:stats": "webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "bootstrap-sass": "^3.4.1",
    "core-js": "^3.1.4",
    "ngx-bootstrap": "^5.0.0",
    "rxjs": "^6.5.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.8",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.1.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "morgan": "^1.9.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.17.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
