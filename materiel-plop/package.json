{
  "name": "materiel-plop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --proxy-config proxy.conf.json\" \"node ./server/main.js\"",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --optimization=false --stats-json",
    "serve:dist": "concurrently \"http-server -g 1 -c-1 -o --proxy http://localhost:8081 -p 9001 ./dist\" \"node ./server/main.js\"",
    "serve:stats": "webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/forms": "^11.2.5",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular-eslint/builder": "2.0.1",
    "@angular-eslint/eslint-plugin": "2.0.1",
    "@angular-eslint/eslint-plugin-template": "2.0.1",
    "@angular-eslint/schematics": "2.0.1",
    "@angular-eslint/template-parser": "2.0.1",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "^11.2.5",
    "@angular/language-service": "^11.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "morgan": "^1.10.0",
    "protractor": "~7.0.0",
    "typescript": "4.0.7",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
