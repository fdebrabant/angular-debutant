{
  "name": "materiel-plop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --proxy-config proxy.conf.json\" \"node ./server/main.js\"",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --optimization=false --stats-json",
    "serve:dist": "concurrently \"http-server -g 1 -c-1 -o --proxy http://localhost:8081 -p 9001 ./dist\" \"node ./server/main.js\"",
    "serve:stats": "webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "bootstrap-sass": "^3.4.1",
    "ngx-bootstrap": "^5.5.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "http-server": "^0.12.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "morgan": "^1.10.0",
    "protractor": "^5.4.3",
    "tslint": "~5.17.0",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
